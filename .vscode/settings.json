{
  "typescript.tsdk": "node_modules/typescript/lib",
  "vite.autoStart": false,
  "xo.enable": true,
  "xo.format.enable": true,
  "editor.defaultFormatter": "samverschueren.linter-xo",
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.defaultFormatter": "samverschueren.linter-xo"
  },
  "files.associations": {
    "*.css": "postcss"
  }
}
